modules = ["bash", "swift-5.8", "ruby-3.2", "python-3.12", "dart-3.8"]
[agent]
expertMode = true

[nix]
channel = "stable-25_05"
packages = ["flutter"]

[workflows]
runButton = "Project"

[[workflows.workflow]]
name = "Project"
mode = "parallel"
author = "agent"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "Flutter Web Server"

[[workflows.workflow]]
name = "Flutter Web Server"
author = "agent"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "flutter run -d web-server --web-hostname 0.0.0.0 --web-port 5000"
waitForPort = 5000

[workflows.workflow.metadata]
outputType = "webview"

[[ports]]
localPort = 5000
externalPort = 80

[[ports]]
localPort = 34277
externalPort = 3002

[[ports]]
localPort = 44307
externalPort = 3000

[deployment]
deploymentTarget = "static"
build = ["flutter", "build"]
publicDir = "build/web"
